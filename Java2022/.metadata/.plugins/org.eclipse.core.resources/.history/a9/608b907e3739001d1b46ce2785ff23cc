package reCapDemo2;

public class Main {

	public static void main(String[] args) {
		double[] myList = {1.2,1.3,4.3,5.6};  //double[] myList = new double[4]; şeklinde de yazılabilir.
		
		for(double number:myList) {
			double total=0;
			System.out.println(number);
			total = total + number;
		}
		System.out.println("Toplam = " + total);
		
		
	}

}
