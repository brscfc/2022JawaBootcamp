package sesliHarfler;

public class Main {

	public static void main(String[] args) {
		char harf = 'A';

		switch (harf) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':
			System.out.println("Kalın sesli harf");
			break; 											//durdur demek
		default:     									   //aksi durumda işlemi görür else gibi.
			System.out.println("İnce sesli harf");

		}

	}
}
